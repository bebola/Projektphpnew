{% extends 'base.html.twig' %}

{% block title %}
    {{ 'title_Categories_details'|trans({'%id%': Categories.id|default('')}) }}
{% endblock %}

{% block body %}
    <h1>{{ 'title_Categories_details'|trans({'%id%': Categories.id|default('')}) }}</h1>
    {% if Categories is defined and Categories|length %}
        <dl class="dl-horizontal">
            <dt>{{ 'label_id'|trans }}</dt>
            <dd>{{ Categories.id }}</dd>
            <dt>{{ 'label_created_at'|trans }}</dt>
            <dd>{{ Categories.createdAt|format_date('long')}}</dd>
            <dt>{{ 'label_updated_at'|trans }}</dt>
            <dd>{{ Categories.updatedAt|format_date('long')}}</dd>
            <dt>{{ 'label_title'|trans }}</dt>
            <dd>{{  Categories.title }}</dd>
        </dl>
        <p>
            <a href="{{ url('Categories_index') }}" title="{{ 'action_back_to_list'|trans }}">
                {{ 'action_back_to_list'|trans }}
            </a>
        </p>
    {% else %}
        <p>
            {{ 'message_item_not_found'|trans }}
        </p>
    {% endif %}
{% endblock %}