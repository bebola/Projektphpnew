{% extends 'base.html.twig' %}

{% block title %}
    {{ 'title_Photos_edit'|trans({'%id%': Photos.id|default('')}) }}
{% endblock %}

{% block body %}
<div id="container3">
    <div id="form">
    <h1>{{ 'title_Photos_edit'|trans({'%id%': Photos.id|default('')}) }}</h1>
    {{ form_start(form, { method: 'PUT', action: url('Photos_edit', {id: Photos.id}) }) }}
    {{ form_widget(form) }}
    <div class="form-group row float-sm-right">
        <input type="submit" value="{{ 'action_save'|trans }}" class="btn btn-primary" />
    </div>
    <div class="form-group row float-sm-left">
        <a href="{{ url('Photos_index') }}" class="btn btn-link">
            {{ 'action_back_to_list'|trans }}
        </a>
    </div>
    </div>
</div>
    {{ form_end(form) }}
{% endblock %}

